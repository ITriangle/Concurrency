package com.wang;

import org.junit.Test;

public class SpringWang {
	private SpringWang(){
		
	}
	
	private static SpringWang springWang = new SpringWang();

	public static SpringWang getSpringWang() {
		return springWang;
	}
	
	@Test
	public void testSpringWang(){
		SpringWang s1 = SpringWang.getSpringWang();
		SpringWang s2 = SpringWang.getSpringWang();
		
		if(s1 == s2){
			System.out.println("s1与s2相同");
			
		}
		else {
			System.out.println("s1与s2不同");
		}
	}
	
}


